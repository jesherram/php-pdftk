{
    "name": "jesherram/php-pdftk",
    "description": "A PDF conversion and form utility based on pdftk.",
    "keywords": ["pdf", "pdftk"],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "jesherram\\pdftk\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "jesherram\\pdftk\\PdfServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Fabiano Mendes"
        }
    ],
    "minimum-stability": "stable",
    "require": {
            "php": ">=7.1",
            "mikehaertl/php-shellcommand": "^1.6.3",
            "mikehaertl/php-tmpfile": "^1.1.0"
    }
}
